    <!-- Welcome fragment -->
    <string name="kannada_welcome_instruction">Welcome. If you are unable to see the following Kannada text then your device is not supported.</string>
    <string name="kannada_welcome_title">ಏನು, ಈ ವಾಕ್ಯ ಕನ್ನಡದಲ್ಲಿ ಸರಿಯಾಗಿ ತೋರುತ್ತದೆಯೋ?</string>
    <!-- Enable Fragment -->
    <string name="kannada_enable_instructions_head">ಸ್ವರಚಕ್ರ ಉಪಯೋಗಿಸುವ ಮೊದಲು ಇದನ್ನು \"ಎನೇಬಲ್ \" ಮತ್ತೆ \"ಡೀಫಾಲ್ಟ್\"  ಮಾಡಬೇಕು.</string>
    <string name="kannada_enable_step_1">೧) ಇನ್ನು ಬರುವ ಸ್ಕ್ರೀನಿನಲ್ಲಿ \"ಸ್ವರಚಕ್ರ ಕನ್ನಡದಲ್ಲಿ\" ಆರಿಸಿ .</string>
    <string name="kannada_enable_step_2">೨) ನಂತರ \"ಓಕೆ\" ದಬಾಯಿಸಿ. </string>
    <string name="kannada_enable_step_3">೩) ಅಂತ್ಯದಲ್ಲಿ ನಿಮ್ಮ ಫೋನಿನ \"ಬಾಕ್ ಬಟನ್ \" ದಬಾಯಿಸಿ.</string>
    <!-- Default Fragment -->
    <string name="kannada_default_instructions_head">ಸ್ವರಚಕ್ರ \"ಕೀಬೋರ್ಡ್\" ಯಶಸ್ವಿಯಾಗಿ \"ಎನೇಬಲ್ \" ಆಗಿದೆ . ಈಗ ಅದನ್ನು ನಿಮ್ಮ  \"ಡೀಫಾಲ್ಟ್ ಕೀಬೋರ್ಡನ್ನಾಗಿ \" ಮಾಡಿ .</string>
    <string name="kannada_default_step_1">೧) ಬರುವ ಸ್ಕ್ರೀನಿನಲ್ಲಿ ಸ್ವರಚಕ್ರ ಕನ್ನಡ ಆರಿಸಿ .</string>
    <!-- Button Text -->
    <string name="kannada_welcome_button">ಹೌದು, ಸರಿಯಾಗಿದೆ.</string>
    <string name="kannada_enable_button">\"ಎನೇಬಲ್\" ಸ್ವರಚಕ್ರ </string>
    <string name="kannada_default_button">ಸ್ವರಚಕ್ರ ವನ್ನು \"ಡೀಫಾಲ್ಟ್\" ಮಾಡಿ.</string>
    <!-- Congrats fragment -->
    <string name="kannada_congrats_instruction">ಸ್ವರಚಕ್ರ ಈಗ ಉಪಯೋಗಿಸಲು ತಯಾರಾಗಿದೆ.</string>
    <string name="kannada_congrats_title">ಧನ್ಯಾವಾದ</string>
    <string name="kannada_congrats_button">ಸೆಟ್ಟಿಂಗ್ಸ್</string>
