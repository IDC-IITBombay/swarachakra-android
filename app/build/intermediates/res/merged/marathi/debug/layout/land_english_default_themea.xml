<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0.75%p"
    android:keyHeight="11%p"
    android:keyWidth="9.25%p"
    android:verticalGap="1.1875%p" >

    <Row
        android:keyHeight="0.78125%p"
        android:verticalGap="0%p" >
    </Row>

    <Row>

        <Key
            android:codes="1"
            android:horizontalGap="0.875%p"
            android:keyEdgeFlags="left"
            android:keyLabel="q" />

        <Key
            android:codes="2"
            android:keyLabel="w" />

        <Key
            android:codes="3"
            android:keyLabel="e" />

        <Key
            android:codes="4"
            android:keyLabel="r" />

        <Key
            android:codes="5"
            android:keyLabel="t" />

        <Key
            android:codes="6"
            android:keyLabel="y" />

        <Key
            android:codes="7"
            android:keyLabel="u" />

        <Key
            android:codes="8"
            android:keyLabel="i" />

        <Key
            android:codes="9"
            android:keyLabel="o" />

        <Key
            android:codes="10"
            android:keyEdgeFlags="right"
            android:keyLabel="p" />
    </Row>

    <Row>

        <Key
            android:codes="11"
            android:horizontalGap="5.875%p"
            android:keyEdgeFlags="left"
            android:keyLabel="a" />

        <Key
            android:codes="12"
            android:keyLabel="s" />

        <Key
            android:codes="13"
            android:keyLabel="d" />

        <Key
            android:codes="14"
            android:keyLabel="f" />

        <Key
            android:codes="15"
            android:keyLabel="g" />

        <Key
            android:codes="16"
            android:keyLabel="h" />

        <Key
            android:codes="17"
            android:keyLabel="j" />

        <Key
            android:codes="18"
            android:keyLabel="k" />

        <Key
            android:codes="19"
            android:keyEdgeFlags="right"
            android:keyLabel="l" />
    </Row>

    <Row>

        <Key
            android:codes="@string/shift"
            android:horizontalGap="1.125%p"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/shift"
            android:keyWidth="11.875%p" />

        <Key
            android:codes="20"
            android:horizontalGap="2.875%p"
            android:keyLabel="z" />

        <Key
            android:codes="21"
            android:keyLabel="x" />

        <Key
            android:codes="22"
            android:keyLabel="c" />

        <Key
            android:codes="23"
            android:keyLabel="v" />

        <Key
            android:codes="24"
            android:keyLabel="b" />

        <Key
            android:codes="25"
            android:keyLabel="n" />

        <Key
            android:codes="26"
            android:keyLabel="m" />

        <Key
            android:codes="@string/backspace"
            android:horizontalGap="2.875%p"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/delete"
            android:keyWidth="11.875%p" />
    </Row>

    <Row android:rowEdgeFlags="bottom" >

        <Key
            android:codes="@string/symbols"
            android:horizontalGap="0.875%p"
            android:keyEdgeFlags="left"
            android:keyLabel="123"
            android:keyWidth="13.5%p" />

        <Key
            android:codes="@string/english"
            android:horizontalGap="1.5%p"
            android:keyLabel="Hn"
            android:keyWidth="9.25%p" />

        <Key
            android:codes="@string/space"
            android:horizontalGap="1.75%p"
            android:keyWidth="46.25%p"
            android:keyLabel="" />

        <Key
            android:codes="53"
            android:horizontalGap="1.75%p"
            android:keyLabel="."
            android:keyWidth="9.25%p" />

        <Key
            android:codes="@string/enter"
            android:horizontalGap="1.5%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_return"
            android:keyWidth="13.5%p" />
    </Row>

    <Row
        android:keyHeight="1.484375%p"
        android:verticalGap="0%p" >
    </Row>

</Keyboard>